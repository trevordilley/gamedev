{
  "name": "apps/kaboom-test",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/kaboom-test/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/kaboom-test",
        "compiler": "swc",
        "main": "apps/kaboom-test/src/main.ts",
        "tsConfig": "apps/kaboom-test/tsconfig.app.json",
        "webpackConfig": "apps/kaboom-test/webpack.config.js",
        "assets": [
          "apps/kaboom-test/src/favicon.ico",
          "apps/kaboom-test/src/assets"
        ],
        "index": "apps/kaboom-test/src/index.html",
        "baseHref": "/",
        "styles": ["apps/kaboom-test/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "apps/kaboom-test/src/environments/environment.ts",
              "with": "apps/kaboom-test/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "apps/kaboom-test:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "apps/kaboom-test:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/kaboom-test"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/kaboom-test/**/*.ts"]
      }
    }
  }
}
